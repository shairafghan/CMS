<h1>Log in</h1>
<%= form_for(:receptionist_session,url: login_receptionist_path) do |f| %>
<%= f.label :email %>
<%= f.email_field :email,class: 'form-control' %>
<%= f.label :password %>
<%= f.password_field :password,class: 'form-control' %>
<%= f.submit "Log in",class: "btn btn-primary" %>
<% end %>
<p> or <%= link_to "Sign up now!",signup_receptionist_path %></p>
